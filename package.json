   "scripts": {
      "build": "rollup -c ",
      "android:build": "cd android && fastlane deploy_qa_release && cd ..",
      "ios:build": "cd ios && fastlane qa_firebase && cd ..",
      "ios:install": "cd ios && pod install && cd ..",
      "android": "react-native run-android",
      "ios": "react-native run-ios",
      "start": "react-native start",
      "test": "jest",
      "lint": "eslint .",
      "prepare": "patch-package",
      "copy_pack": "cp -r dist/* /home/user/Web_Training/React_Native/SamplePoc/node_modules/mirrorfly/dist/",
      "build:pack:copy": "npm run build && npm pack && cp -r mirrorfly-uikit-react-native-1.3.0.tgz /Users/user/Desktop/mirrorfly/rn/sampleproject_0_69_12",
      "i": "npm i -f && npm run ios:install",
      "android:clean": "cd android && ./gradlew clean && rm -rf build && cd ..",
      "ios:clean": "rm -rf ios/Pods/ ios/Podfile.lock ios/build && rm -rf ~/Library/Developer/Xcode/DerivedData",
      "file:clean": "rm -rf package-lock.json node_modules && npm run ios:clean",
      "rn:clean": "npm run android:clean && watchman watch-del-all && npm run ios:clean && npm run file:clean",
      "patch:zip": "zip -r patches.zip ./patches",
      "watchman-clean": "watchman watch-del '/Users/user/Desktop/mirrorfly/rn/rn_0.72/p078-mirrorfly-reactnative-uikitsdk' ; watchman watch-project '/Users/user/Desktop/mirrorfly/rn/rn_0.72/p078-mirrorfly-reactnative-uikitsdk'"
